<p id="notice"><%= notice %></p>

<p>
  <b>Text:</b>
  <%= @question.text %>
</p>

<% if @question.alternatives %>
<p>
  <b>Alternatives:</b>
  <%= @question.alternatives %>
  </p>
<% end %>

<p>
  <b>Answers:</b>
  <%= @question.answers %>
</p>

<p>
  <b>Extra:</b>
  <%= @question.extra %>
</p>


<%= link_to 'Edit', edit_question_path(@question) %> |
<%= link_to 'Back', questions_path %>
