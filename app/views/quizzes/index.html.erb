<h1>Listing quizzes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Default theme</th>
    <th></th>
  </tr>

<% @quizzes.each do |quiz| %>
  <tr>
    <td><%= quiz.name %></td>
    <td><%= quiz.default_theme %></td>
    <td>
      <%= link_to 'Show', quiz %> |
      <%= link_to 'Edit', edit_quiz_path(quiz) %> |
      <%= link_to 'Destroy', quiz, method: :delete, data: { confirm: 'Are you sure?' } %>
  </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quiz', new_quiz_path %>
